{{#each items}}

<li>
  <div class="item">
    <div class="item_cart">
      <button data-action="ADD_TO_CART" data-id="{{_id}}" class="item_button">
        Add to cart
      </button>
    </div>
    <div class="item_imgContainer">
      <img
        src="{{image}}"
        class="item_img" width="250px">
    </div>
    <p class="item_name">{{brandName}}</p>
    <p class="item_desc">{{name}}</p>
    <p class="item_price">$ {{price}}</p>
  </div>
</li>

{{/each}}
